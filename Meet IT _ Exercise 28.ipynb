    "import torch\n",
    "import numpy as np\n",
    "\n",
    "x = torch.autograd.Variable(torch.Tensor([np.pi]),requires_grad=True)\n",
    "y = torch.tan(x) + 9*torch.cos(x)\n",
    "\n",
    "y.backward()\n",
    "x.grad"
   ]
